{
  "compilerOptions": {
    "outDir": "./worker",
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext"],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "types": ["@cloudflare/workers-types"]
  },
  "exclude": ["node_modules/", "dist/"]
}
